const mongoose = require('mongoose');

mongoose
  .connect(
    'mongodb+srv://touiteur:touiteur@cluster.junge.mongodb.net/touiteur?retryWrites=true&w=majority',
    {
      useNewUrlParser: true,
      useUnifiedTopology: true,
      useFindAndModify: false,
      useCreateIndex: true,
    })
  .then(() => console.log('Connected to db.'))
  .catch(console.error);
